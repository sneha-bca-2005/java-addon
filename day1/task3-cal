package day1;

public class calculater {

	public static void main(String[] args) {
		int a=10;
		int b=20;
		int c=30;
		int d=40;
	
	
	int cal = 0;
	switch(cal) {
	case 0:
	System.out.println(a+b);
	
	case 1:
		System.out.println(a-b);
		break;
	case 2:
		System.out.println(b*d);
		break;
	case 3:
		System.out.println(c/a);
		
	
 default :

		System.out.println("successfully");

}
	}

	
	}
