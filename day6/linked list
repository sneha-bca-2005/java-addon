import java.util.LinkedList;
public class LinkedListClass{
	public static void main(String[] args) {
		


LinkedList<Object> vn=new LinkedList<Object>();
vn.remove("sneha");

vn.add("A");
vn.add("computer application");
vn.add("3 year");
vn.add(0,"HARI");
vn.set(0,"john");
System.out.println(vn.get(1));
vn.forEach(date-> System.out.println(date));


}
}
