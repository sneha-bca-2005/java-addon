import java.util.ArrayList;
public class ArrayListClass{
	public static void main(String[] args) {
		ArrayList<Object> arr=new ArrayList<Object>();
arr.add("sneha");
arr.add("A");
arr.add("computer application");
arr.add("3 year");
arr.add(0,"HARI");
arr.add(0,"john");
arr.forEach(date-> System.out.println(date));
   }
}
