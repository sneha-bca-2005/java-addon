import java.util.*;



	
	   class main extends Thread{
	    public void run(){
	        for(int i=0;i<=10;i++){
	        System.out.println("haii thread"+" "+i);
	    }
	    }
	}
	class execute{
	    public static void main(String[] args){
	        main m=new main();
	        m.start();
	    }
	}

