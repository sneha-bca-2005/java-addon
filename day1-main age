import java.util.*;
class main{
    public static void main(String[] args){
        System.out.println("enter the age");
        Scanner ref=new Scanner(System.in);
        int a=ref.nextInt();
        System.out.println(a);
        
        if(a>18){
            System.out.println("you are eligible");
        }
        else
    {
        System.out.println("you are not eligible");
    }
        
        }
    }
