

class Main {
    public static void main(String[] args) {
        char a;
        char b;
        char c;
        char d;
        char e;
     char grade=1;
     switch(grade){ 
         case 1:
        System.out.println("excellent");
         
        case 2:
        System.out.println("good");  
        
        case 3:
        System.out.println("average");
        


       case 4:
        System.out.println("poor");
        
     
      case 5:
        System.out.println("fail");
       
       default :
      System.out.println("invalid");
      
      
     }
    }
        
    }
      
      
