import java.util.*;
class main{
    public static void main(String[] args){
        Scanner in=new Scanner(System.in);
        try{
         System.out.println("enter the number:");    
       int num=in.nextInt();
       int total=10/num;
       System.out.println("divide by zero:"+total);
        
        }
        catch(Exception e){
             System.out.println("divide by zero:"+e);
        }
    }
}
